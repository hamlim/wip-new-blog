    "@tailwindcss/vite": "4.1.4",
    "@toolwind/anchors": "0.0.11",
    "@types/bun": "1.2.10",
    "@types/node": "22.13.0",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vercel/og": "0.6.8",
    "concurrently": "9.1.2",
