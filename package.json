    "rehype-mdx-code-props": "3.0.1",
    "remark-flexible-markers": "1.2.1",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.1",
    "remark-mdx-frontmatter": "5.0.0",
    "satori": "0.12.2",
    "tailwindcss": "4.1.4",
    "tinyglobby": "0.2.13",
    "tw-animate-css": "^1.2.9",
    "typescript": "5.8.3"
  }
